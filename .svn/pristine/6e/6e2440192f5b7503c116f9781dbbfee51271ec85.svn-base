
.homepage-more {
    float: right;
}

.homepage-more:hover {
    cursor: pointer;
}

.layui-card-body {
    height: 70%;
}

.layui-card-body dl {
    height: 100%;
}

.layui-card-body dl dd {
    width: 100%;
    height: 30px;
    line-height: 30px;
}

.layui-card-body1 dl {
    height: 10%;
}

.layui-card-body1 dl dd {
    margin: 3px;
    width: 100%;
}

dd:hover {
    cursor: pointer;
    background-color: #c4e3f3;
}

.homepage-title {
    width: 70%;
    float: left;
    padding-left: 10px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.homepage-name {
    width: 10%;
    float: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.homepage-time {
    width: 20%;
    float: left;
    text-align: right;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.homepage-plan {
    width: 70%;
    float: left;
    margin: 5px 5px 5px 20px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.homepage-plan:hover {
    cursor: default;
}

.homepage-background {
    background-color: #c4e3f3;
}

.homepage-space > * {
    padding: 3.3px
}

.homepage-card {
    padding: 3.3px 3.3px 0 3.3px;
    background-color: #c4e3f3;
}

.oa-system-link {
    text-align: center;
    cursor: pointer;
}

.oa-system-link:hover {
    background-color: #c4e3f3;
}

.oa-homepage-birthday {
    margin-left: 5px;
    text-align: center;
    cursor: default;
}
.layui-card-header{
    cursor:default;
}
