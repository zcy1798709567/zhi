/*功能*/
.function-wrap{
	width: 100%;
	height: auto;
	overflow: hidden;
	padding:0.4rem;
}
a.function-list{
	display: block;
	width: 4.4133rem;
	height: 2.28rem;
	overflow: hidden;
	margin-bottom: 0.4rem;
	position: relative;
}
a.function-list:nth-child(odd){
	float: left;
}
a.function-list:nth-child(even){
	float: right;
}
a.function-list img{
	width: 100%;
}
a.function-list label{
	position: absolute;
	top: 20px;
	left: 10px;
}
