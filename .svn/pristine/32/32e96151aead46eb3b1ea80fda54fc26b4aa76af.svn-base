	right: -12px;
}
.schedule-title{
	color: #28D8A1;
	font-size: 16px;
	font-weight: bold;
}
.schedule-line:after{
	content: "";
	width: 25px;
	height: 3px;
	background: #28D8A1;
	position: absolute;
	bottom: -2px;
	left: 15px;
}
.schedule-icon-span{
	display: block;
	width: 8px;
	height: 8px;
	overflow: hidden;
	background:#28D8A1;
	border-radius: 50%;
	-wekit-border-radius: 50%;	
	display: none;
}
a.schedule-list:active.my-bg-line{
	background: #f8f8f8;
}
a.schedule-list:active .schedule-icon-span{
	display:block;
}
.schedule-panel{
	width: 100%;
	height: auto;
	overflow: hidden;
	padding:10px 0;
	background: #fff;
}
