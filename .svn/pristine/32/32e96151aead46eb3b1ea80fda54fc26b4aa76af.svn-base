/*任务详情*/
.task-header{
	background:#fff;
	padding:10px 15px 0px;
}
.task-header-icon{
	width: 14px;
	height: 14px;
	background:url(../images/jt1.png) no-repeat center;
	background-size:14px auto;
}
.task-header.collapsed .task-header-icon{
	background:url(../images/jt2.png) no-repeat center;
	background-size:14px auto;	
}
.task-header.collapsed{
	padding:10px 15px;	
}
.task-header .task-tit{
	opacity: 0;
}
.task-header.collapsed .task-tit{
	opacity: 1;
}
.task-body-title{
	text-align: center;
	font-size: 16px;
	color: #28D8A1;
	font-weight: bold;
	position: relative;
	margin-left: 5px;
    margin-right: 5px;
}
.task-body-title:before{
	content: "";
	position: absolute;
	width: 30%;
	height: 1px;
	background:#28D8A1;
	left: 0;
	top:50%;
}
.task-body-title:after{
	content: "";
	position: absolute;
	width: 30%;
	height: 1px;
	background:#28D8A1;
	right: 0;
	top:50%;
}
.task-body-content{
	padding:10px;
	border-bottom: solid 1px #F2F2F2;
}
.circumstance-title{
	color: #B3B3B3;
	width: 84px;
	text-align: right;
	align-self: self-start;
}
.circumstance-bar{
	padding: 5px 0;
}
.task-body-footer{
	height: 40px;
	line-height: 40px;
	text-align: right;
	padding:0 20px;
}
a.circumstance-see{
	color: #6C8EDF;
}
.objection-title{
    width: 74px;
    text-align: right;
    align-self: self-start;	
}
.detail-bg-line{
    width: 100%;
    padding: 7px 15px;
    background: rgba(255,255,255,1);
    position: relative;	
}
.detail-bg-line:after{
    content: ""; 
    display: block; 
    height: 0; 
    clear: both; 
    visibility: hidden;
}
.objection-right{
	position: relative;
	padding-bottom: 8px;
	padding-left: 8px;
}
.objection-right:after{
	content: "";
	position: absolute;
	width: 100%;
	height: 1px;
	background:#F2F2F2;
	left: 0;
	bottom: 0;
}
.objection-select{
	width: 100%;
	border:none;
	-webkit-appearance: none;
}
.enclosure-btn a{
	color: #28D8A1;
}
.my-mission-list:hover .enclosure-btn a, .my-mission-list:active .enclosure-btn a,.enclosure-bar.on .enclosure-btn a{
