@charset "utf-8";
/*会议管理*/
.box-bar{
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -moz-box-orient: horizontal;
    -moz-box-direction: normal;
    -webkit-flex-direction: row;
    -ms-flex-direction: row;
    flex-direction: row;
}
.box-bar-list{
    -webkit-box-flex: 1;
    -moz-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
    box-sizing: border-box;
    align-self: center;
}
.cell{
    -webkit-box-flex: 0;
    -webkit-flex: none;
    -ms-flex: none;
    flex: none;
    width: auto;
    align-self: center;
}
.pic-banner{
    width:100%;
}
.foot-bar p{
    display: block;
    width:990px;
    height: 62px;
    line-height: 32px;
    box-sizing: border-box;
    text-align: left;
    float: left;
    color:#A5A5A5;
    padding-top:15px;
    padding-bottom:15px;
}
.card-panel{
    border-top:4px solid #4B8CEE;
}
a.index-more{
    font-size:12px;
    color:#A0A0A0;
}
a.index-more:hover{
    color:#B71111;
    cursor: pointer;
    -webkit-transition: all 0.3s ease-out 0s;
    -moz-transition: all 0.3s ease-out 0s;
    -ms-transition: all 0.3s ease-out 0s;
    -o-transition: all 0.3s ease-out 0s;
    transition: all 0.3s ease-out 0s;
}
.slide-wrap{
    position: relative;
}
.slide-infor{
    width: 100%;
    height: 40px;
    line-height: 40px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    color:#fff;
    padding:0 20px;
    overflow: hidden;
    position: absolute;
    left: 0;
    bottom: 0;
    background:rgba(34,105,209,0.8);
    white-space:nowrap;
    text-overflow:ellipsis;
    padding-right:120px;
}
.layui-carousel-ind{
    text-align: right;
    padding-right: 20px;
}
.layui-carousel-ind ul{
    background:none;
}
.layui-carousel-ind li.layui-this{
    background:#E8332F;
}
.layui-carousel-arrow:hover, .layui-carousel-ind ul:hover{
    background:none;
}
.layui-carousel-ind li{
    width: 26px;
    height: 7px;
    background:#fff;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    border-radius: 0;
}
.layui-carousel-ind{
    top:-28px;
}
a.list-bar{
    color: #666;
    height: 35px;
    line-height: 35px;
}
a.list-bar:hover{
    color:#FF7300;
    -webkit-transition: all 0.3s ease-out 0s;
    -moz-transition: all 0.3s ease-out 0s;
    -ms-transition: all 0.3s ease-out 0s;
    -o-transition: all 0.3s ease-out 0s;
    transition: all 0.3s ease-out 0s;
}
a.list-bar .box-bar-list,a.task-list-bar .box-bar-list{
    overflow: hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
    margin-right: 10px;
}
.hd-header{
    font-size: 16px;
    color:#B71111;
    border-bottom: solid 2px #B71111;
}
.hd-header.orenge{
    border-bottom: solid 2px #F9AA00;
}
.message-icon{
    margin-right: 10px;
}
a.task-list-bar{
    color: #666;
    height: 42px;
    line-height: 42px;
    border-bottom:dotted 1px #EEEFF3;
}
.mar-r{
    margin-right: 10px;
}
.affairs-hd{
    border-bottom: 3px #D52424 solid;
    color:#D52424;
}
li.menu-list{
    width: 20%;
    padding: 5px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    float: left;
}
.menu-list-a{
    display: block;
    width:100%;
    height: 100%;
}
.menu-view{
    min-height: 210px;
    height: auto;
    overflow: hidden;
    padding-top:10px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.menu-imgbox img{
    display: block;
    margin:0 auto;
}
.menu-infor{
    text-align: center;
    overflow: hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
}
.system-icon{
    position: relative;
    width: 3px;
    height: 36px;
}
.system-icon:after{
    content: "";
    position: absolute;
    width: 3px;
    height: 20px;
    background: #8F83F2;
    left:-15px;
    top:8px;
}
.system-icon.bgg:after{
    background: #8EC864;
}
.system-body{
    min-height: 160px;
}
/*质检院首页*/
.out-main-fluid{
    background:url(../images/bg.png) no-repeat center top;
    -webkit-background-size: 100% auto;
    background-size: 100% auto;
}
.main-fluid{
    width:1400px;
    height: auto;
    overflow: hidden;
    margin:0 auto;
}
.img-logo{
    margin-top:20px;
}
.nav-bar{
    width: 100%;
    height: 48px;
    overflow: hidden;
    background:#E8332F;
}
.nav-li{
    display: inline-block;
}
a.nav-a{
    display: inline;
    color: #F9AFAF;
    display: inline-block;;
    line-height: 48px;
    padding:0 28px;
    font-size: 16px;
    -webkit-transition: all 0.3s ease-out 0s;
    -moz-transition: all 0.3s ease-out 0s;
    -ms-transition: all 0.3s ease-out 0s;
    -o-transition: all 0.3s ease-out 0s;
    transition: all 0.3s ease-out 0s;
}
.on a.nav-a,a.nav-a:hover{
    background:#B71111;
    color: #fff;
}
.out-col8{
    text-align: center;
    padding: 25px 0;
}
.col8-span{
    background-position: left center;
    background-repeat: no-repeat;
    -webkit-background-size: 43px;
    background-size: 43px;
    padding-left: 53px;
    padding-top: 25px;
    padding-bottom: 25px;
}
.spa1{
    background-image: url(../images/ic1.png);
}
.spa2{
    background-image: url(../images/ic2.png);
}
.spa3{
    background-image: url(../images/ic3.png);
}
.spa4{
    background-image: url(../images/ic4.png);
}
.spa5{
    background-image: url(../images/ic5.png);
}
.spa6{
    background-image: url(../images/ic6.png);
}
.spa7{
    background-image: url(../images/ic7.png);
}
.spa8{
    background-image: url(../images/ic8.png);
}
/*图文混排*/
a.tu-wen-panel{
    display: block;
    position: relative;
    width: 100%;
    height: auto;
    overflow: hidden;
}
a.tu-wen-panel:hover .wen-panel-hd,a.tu-wen-panel:hover .wen-panel-bd{
    color:#FF7300;
    -webkit-transition: all 0.3s ease-out 0s;
    -moz-transition: all 0.3s ease-out 0s;
    -ms-transition: all 0.3s ease-out 0s;
    -o-transition: all 0.3s ease-out 0s;
    transition: all 0.3s ease-out 0s;
}
.tu-imgbox{
    width: 151px;
    height: 80px;
    overflow: hidden;
    position: absolute;
    z-index: 1;
    left: 0;
    top:0;
}
.tu-imgbox img{
    width: 100%;
    transition: all 0.6s cubic-bezier(0.215, 0.61, 0.355, 1) 0s;
}
a.tu-wen-panel:hover .tu-imgbox img{
    transform: scale(1.08, 1.08);
}
.wen-panel{
    padding-left: 170px;
}
.wen-panel-hd{
    color: #5B5959;
    height: 26px;
    line-height: 26px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.wen-panel-bd{
    color:#9E9E9E;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    line-clamp: 2;
    -webkit-box-orient: vertical;

}
.footer-imgbox{
    width: 410px;
    height: 100%;
    overflow: hidden;
    float: left;
}
.footer-imgbox img{
    margin-right: 20px;
    float: left;
}
/*友情链接*/
.friend-link-wrap{
    background:url(../images/hill.png) no-repeat center right #fff;
    background-size:467px auto;
    padding:20px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding-right: 380px;
    height: auto;
    overflow: hidden;
}
.friend-link-title{
    font-size: 16px;
    color:#B71111;
    font-weight: bold;
    width: 36px;
    float: left;
    margin-right: 10px;
    margin-top: 5px;
}
.friend-link-jt{
    width: 50px;
    height: 55px;
    overflow: hidden;
    float: left;
}
/* apply */
.friend-link-view{
    width:824px;
    float: left;
    margin-right: 10px;
}
.apply_array{width:194px;float:left;height:55px;
    overflow: hidden;margin-right:12px;}
.apply_array:hover img{
    transform: scale(1.08, 1.08);
    -webkit-transition: all 0.3s ease-out 0s;
    -moz-transition: all 0.3s ease-out 0s;
    -ms-transition: all 0.3s ease-out 0s;
    -o-transition: all 0.3s ease-out 0s;
    transition: all 0.3s ease-out 0s;
}
.apply_array .vote{position:absolute;line-height:26px;margin-top:10px;padding-left:29px;width:83px;display:block;background:url(imgs/vote.gif) no-repeat 0px 0px;height:26px;color:#282828;margin-left:-6px;text-decoration:none;}
.apply_last{margin-right:0px}
.apply_img{text-align:center;width:100px;display:table-cell;background:#e2e2e2;height:100px;font-size:95px;vertical-align:middle}
.apply_array a{text-decoration:underline}
.apply_array a:hover{color:#e01111}
.apply_info{text-align:center;padding:15px;}
.apply_comment{text-align:center}
.apply_info a{line-height:17px;font-weight:700}
.apply_nav{position:relative;width:824px;float:left;height:55px;overflow:hidden}
.apply_w{position:absolute;float:left;left:0px}
.img_l{width:22px;height: 55px;float:left;cursor:pointer;}
.img_r{width:22px;height: 55px;float:right;cursor:pointer;}
.img_l:hover,.img_r:hover{
    opacity: 0.6;
}

/*列表頁*/
.main-view-container{
    height: auto;
    overflow: hidden;
    padding-left:25px;
    padding-right:25px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.crumbs-bar{
    color: #BAB9B9;
    font-size: 12px;
    margin-bottom: 10px;
}
.list-wrap{
    height: auto;
    overflow: hidden;
    padding-bottom: 70px;
}
.left-part-wrap{
    width:260px;
    height: auto;
    overflow: hidden;
    background:#EEEFF3;
    float: left;
}
.left-part-hd{
    width:100%;
    height: 41px;
    line-height: 41px;
    overflow: hidden;
    background:url(../images/hd1.png) no-repeat center;
    background-size:cover;
    color:#fff;
    text-align: center;
}
.left-part-bd{
    padding:10px 0 88px;
}
.left-part-li{
    height: 40px;
    line-height: 40px;
    border-bottom: dotted 1px #B2B2B2;
    padding-left: 20px;
}
.left-part-li a{
    padding-left: 20px;
    display: block;
    width:100%;
    height: 100%;
}
.left-part-li a:hover,.left-part-li.on a{
    color:#E8332F;
    background:url(../images/dot.png) no-repeat left center;
    -webkit-transition: all 0.3s ease-out 0s;
    -moz-transition: all 0.3s ease-out 0s;
    -ms-transition: all 0.3s ease-out 0s;
    -o-transition: all 0.3s ease-out 0s;
    transition: all 0.3s ease-out 0s;
}
/*右侧*/
.right-part-wrap{
    width:1060px;
    height: auto;
    overflow: hidden;
    float: right;
}
.right-part-li{
    height: 36px;
    line-height: 36px;
    overflow: hidden;
}
.right-part-a{
    display: block;
    width: 100%;
    height: 100%;
    color:#727272;
}
.right-part-a:hover{
    color:#FF7300;
    -webkit-transition: all 0.3s ease-out 0s;
    -moz-transition: all 0.3s ease-out 0s;
    -ms-transition: all 0.3s ease-out 0s;
    -o-transition: all 0.3s ease-out 0s;
    transition: all 0.3s ease-out 0s;
}
.right-part-con{
    width:85%;
    float: left;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.right-part-data{
    width:15%;
    float: left;
    text-align: right;
}
.page-wrap{
    margin-top:20px;
}
.page-wrap .layui-laypage .layui-laypage-curr .layui-laypage-em{
    background-color: #E8332F;
}
.page-wrap .layui-laypage input:focus,.page-wrap .layui-laypage select:focus{
    border-color: #E8332F !important;
}
.page-wrap .layui-laypage a:hover{
    color:#E8332F;
}
/*详情*/
.first-last-page{
    background:#EEEFF3;
    padding:0 30px;
    height: 40px;
    line-height: 40px;
    overflow: hidden;
    border-top:dotted 1px #B2B2B2;
}
a.first-last-a{
    display: block;
    width:100%;
    height: 40px;
    line-height: 40px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
a.first-last-a:hover{
    color:#E8332F;
    -webkit-transition: all 0.3s ease-out 0s;
    -moz-transition: all 0.3s ease-out 0s;
    -ms-transition: all 0.3s ease-out 0s;
    -o-transition: all 0.3s ease-out 0s;
    transition: all 0.3s ease-out 0s;
}
.detail-wrap{
    width:100%;
    height: auto;
    overflow: hidden;
    margin-bottom: 30px;
}
.detail-hd{
    text-align: center;
    border-bottom: solid 6px #EEEFF3;
}
.detail-title{
    font-size: 20px;
    color: #000;
    margin-bottom: 5px;
}
.detail-data{
    color:#727272;
    margin-bottom: 5px;
}
.detail-bd{
    padding:10px;
    height: auto;
    overflow: hidden;
}



















