    <script type='text/javascript' src='/views/common/css.js' charset='utf-8'></script>
    <script type='text/javascript' src='/views/common/up-js.js' charset='utf-8'></script>
    <title>我的页面</title>
    <style>
        .layui-m-layercont {
            padding: 0px 0px;
            line-height: 22px;
            text-align: center;
        }
    </style>
</head>
<body>
<!-- 头部begin -->
<div class="header-bar box-bar">
    <a href="javascript:navigationPoP();" class="header-bar-left cell"><i class="iconfont">&#xe606;</i></a>
    <div class="header-bar-title">表单填写</div>
    <!--<a href="javascript:;" class="header-bar-right cell">查看流程图</a>-->
</div>
<!-- 头部end -->
<!-- 底部begin -->
<div class="footer-oper-bar box-bar">
    <div class="box-bar-list mar-r">
        <a href="javascript:formsave();" class="footer-oper-save">保存</a>
    </div>
    <div class="box-bar-list">
        <a href="javascript:navigationPoP();" class="footer-oper-cancel">取消</a>
    </div>
</div>
<!-- 底部end -->
<div class="marin-wrap">
    <div class="my-wrap">
        <div class="my-mission pad-tb mar-b">
            <div class="task-body-title" id="formtitle"></div>
            <div class="my-wrap">
                <form id="formtable" action="" method="POST" novalidate="novalidate"></form>
                <input type="hidden" id="sendvalue" value="">
            </div>
        </div>
    </div>
</div>

<div id="template"></div>
<div id="alert-template" hidden></div>

</body>
<script type='text/javascript' src='/views/common/down-js.js' charset='utf-8'></script>
<script>
    $(document).ready(function () {
        $("#template").load("/views/template/formInfo.html", function (content) {
            $("#template").html(content);
            getFieldHtml();
        });
    });
</script>
<script>
    $(".footer-oper-save").on("click", function () {
        layer.open({
            content: '<div style="height: 100px;">保存成功，是否发送任务？</div>'
            ,btn: ['发送', '取消']
            ,yes: function(index){
                formsend(index);
            }
            ,no: function () {
                navigationPoP();
            }
        });
    });

    function formsave(formid) {
        var urlval = manyValues();
        var uid = urlval.userid == null ? "" : urlval.userid;
        var f = urlval.formid == null ? "" : urlval.formid;
        let p = urlval.procId == null ? "" : urlval.procId;
        let wf = urlval.wkflwID == null ? "" : urlval.wkflwID;
        let wn = urlval.wkfNode == null ? "" : urlval.wkfNode;
        let wo = urlval.workOrderNO == null ? "" : urlval.workOrderNO;
        let re = urlval.recno == null ? "" : urlval.recno;
        $("#formtable").attr("action","/weixin/flowpage/flowaddsave/"+f+".do?userid="+uid+"&procId="+p+"&wkflwId="+wf+"&nodeId="+wn+"&workOrderNO="+wo);
        $("#formtable").ajaxSubmit(function(data) {
            console.log(JSON.stringify(data.data))
            $("#sendvalue").val(JSON.stringify(data.data));
        });
        return false;
    }

    function formsend(index) {
        var urlval = manyValues();
        var result = $.parseJSON($("#sendvalue").val());
        console.log(result)
        var uid = urlval.userid == null ? "" : urlval.userid;
        var f = urlval.formid == null ? "" : urlval.formid;
        let p = result.procId == null ? "" : result.procId;
        let wf = result.wkflwId == null ? "" : result.wkflwId;
        let wn = result.wkfNode == null ? "" : result.wkfNode;
        let wo = result.workOrderNO == null ? "" : result.workOrderNO;
        let re = result.recno == null ? "" : result.recno;
        console.log(re)
        $.ajax({
            type: "POST",
            url: "/weixin/flowpage/flowformsend/" + f + ".do?userid="+uid+"&recno=" + re + "&procId=" + p + "&wkflwId=" + wf + "&wkfNode=" + wn + "&workOrderNO=" + wo,
            data: result,
            dataType: "json",
            success: function (data) {
                if (data.success == "1") {
                    navigationPoP();
                }
                console.log(data)
            },
            error: function (message) {
                console.log(message)
            }
        });
    }
</script>
</html>
